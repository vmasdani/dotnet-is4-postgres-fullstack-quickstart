{
  "ConnectionStrings": {
    "PostgresUrl": "Host=#{host_url};Port=#{postgres_port};Username=#{postgres_user};Password=#{postgres_password};Database=dotnetproject",
    "HostUrl": "#{host_url}",
    "IdentityServerPort": "#{is4_sts_port}",
    "SkorubaClientId": "#{skoruba_client_id}",
    "SkorubaClientSecret": "#{skoruba_client_secret}"
